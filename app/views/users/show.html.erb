<h1>Users#show</h1>
<%= @user.username %>

<% if user_signed_in? %>
  <%= link_to "Log Out", logout_path, data: {
                                 turbo_method: :delete, 
                                 turbo_confirm: "Are you sure you want to log out?" 
                                 } %>
<% else %>
  <%= link_to "Log In", login_path %> |
  <%= link_to "Sign Up", signup_path %>
<% end %>